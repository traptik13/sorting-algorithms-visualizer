{
  "services": [
    {
      "name": "frontend",
      "root": "frontend",
      "startCommand": "npx serve"
    },
    {
      "name": "backend",
      "root": "backend",
      "buildCommand": "javac -cp \".:gson-2.8.9.jar:mysql-connector-j-9.3.0.jar\" *.java",
      "startCommand": "java -cp \".:gson-2.8.9.jar:mysql-connector-j-9.3.0.jar\" MainServer",
      "env": {
        "PORT": "8081"
      }
    }
  ]
}
